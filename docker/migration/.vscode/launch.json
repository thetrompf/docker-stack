{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach Migration (dev)",
            "address": "migration",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/service",
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "sourceMaps": true,
            "smartStep": true,
            "sourceMapPathOverrides": {
                "/service/*": "${workspaceFolder}/*"
            }
        }
    ]
}
